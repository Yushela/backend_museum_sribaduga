{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install --legacy-peer-deps"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ]
}
